@model JPP.Domain.Contactpersoon

@{
    ViewBag.Title = "Contactpersoon details";
}

@if (User.IsInRole("Admin"))
{
    
    
<h2>Details</h2>

<div>
    <h4>Contactpersoon</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
           Naam
        </dt>

        <dd>
            @Html.DisplayFor(model => model.naam)
        </dd>

        <dt>
           Straat
        </dt>

        <dd>
            @Html.DisplayFor(model => model.straat)
        </dd>

        <dt>
           Gemeente
        </dt>

        <dd>
            @Html.DisplayFor(model => model.gemeente)
        </dd>

        <dt>
           Email
        </dt>

        <dd>
            @Html.DisplayFor(model => model.email)
        </dd>

        <dt>
            Telefoon Nr.
        </dt>

        <dd>
            @Html.DisplayFor(model => model.telefoonNr)
        </dd>

        <dt>
          IP-Address
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ipAddress)
        </dd>

    </dl>
</div>
<p>

    <a href="@Url.Action("Index")"><img src="~/Images/page icons/goBack2.png" /> Terug naar de lijst</a>
</p>

}